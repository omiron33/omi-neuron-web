{
  "name": "@omiron33/omi-neuron-web",
  "version": "0.2.6",
  "description": "A drop-in Next.js library for data analysis and 3D visualization with OpenAI-powered insights",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "omi-neuron": "./dist/cli/index.cjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./visualization": {
      "types": "./dist/visualization/index.d.ts",
      "import": "./dist/visualization/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    },
    "./migration": {
      "types": "./dist/migration/index.d.ts",
      "import": "./dist/migration/index.js"
    }
  },
  "files": [
    "dist",
    "docker",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish",
    "prerelease": "pnpm run test && pnpm run build"
  },
  "keywords": [
    "nextjs",
    "react",
    "threejs",
    "graph",
    "visualization",
    "openai",
    "embeddings",
    "knowledge-graph",
    "neuron-web",
    "data-analysis"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "three": ">=0.150.0"
  },
  "peerDependenciesMeta": {
    "three": {
      "optional": true
    }
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "nanoid": "^5.1.6",
    "openai": "^6.15.0",
    "pg": "^8.16.3",
    "slugify": "^1.6.6",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.27.7",
    "@eslint/js": "^9.39.0",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.182.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^24.0.0",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.27.0",
  "engines": {
    "node": ">=18.0.0"
  }
}
